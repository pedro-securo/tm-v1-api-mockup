{
  "priority": 1,
  "request": {
    "urlPathPattern": "/v3.0/oat/dataPipelines/(.*)",
    "method": "PATCH",
    "headers": {
      "Authorization": {
        "contains": "NOT_FOUND"
      }
    }
  },
  "response": {
    "status": 404,
    "transformers": [
      "response-template"
    ],
    "jsonBody": {
      "error": {
        "code": "NotFound",
        "message": "Verify that the specified data pipeline exists and try again."
      }
    },
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
