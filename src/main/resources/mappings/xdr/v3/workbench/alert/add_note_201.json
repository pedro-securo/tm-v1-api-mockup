{
  "priority": 99,
  "request": {
    "urlPathPattern": "/v3.0/workbench/alerts/.*/notes",
    "method": "POST",
    "bodyPatterns": [
      {
        "matchesJsonPath": "$.content"
      }
    ]
  },
  "response": {
    "status": 201,
    "transformers": [
      "response-template"
    ],
    "headers": {
      "Location": "{{request.baseUrl}}/v3/workbench/alerts/{{request.pathSegments.[3]}}/notes/1"
    }
  }
}