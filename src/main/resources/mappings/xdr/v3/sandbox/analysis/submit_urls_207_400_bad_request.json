{
  "priority": 1,
  "request": {
    "urlPath": "/v3.0/sandbox/urls/analyze",
    "method": "POST",
    "headers": {
      "Content-Type": {
        "contains": "application/json"
      }
    },
    "bodyPatterns": [
      {
        "matchesJsonPath": {
          "expression": "$..url",
          "contains": "bad_request"
        }
      }
    ]
  },
  "response": {
    "status": 207,
    "transformers": [
      "response-template"
    ],
    "jsonBody": {
      "status": 400,
      "body": {
        "url": "test",
        "error": {
          "code": "BadRequest",
          "message": "URL format is not right"
        }
      }
    },
    "headers": {
      "Content-Type": "application/json"
    }
  }
}