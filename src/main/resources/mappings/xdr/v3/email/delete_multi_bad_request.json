{
  "priority": 1,
  "request": {
    "urlPath": "/v3.0/response/emails/delete",
    "method": "POST",
    "bodyPatterns": [
      {
        "matchesJsonPath": {
          "expression": "$..uniqueId",
          "equalTo": "multi_bad_request"
        }
      }
    ]
  },
  "response": {
    "status": 207,
    "transformers": ["response-template"],
    "jsonBody": [
      {
        "status": 400,
        "body": {
          "error": {
            "code": "BadRequest",
            "message": "Invalid request - Required fields not found: %field%."
          }
        }
      }
    ],
    "headers": {
      "Content-Type": "application/json"
    }
  }
}